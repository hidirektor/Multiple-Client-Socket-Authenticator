<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="200.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.t3sl4.socketauthenticator.controllers.AuthenticatorController">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <Button fx:id="loginButtonId" layoutX="134.0" layoutY="139.0" mnemonicParsing="false" onAction="#loginButton" text="Login !" />
                <TextField fx:id="userNameTextField" layoutX="86.0" layoutY="39.0" promptText="Username" />
                <TextField fx:id="passwordTextField" layoutX="85.0" layoutY="75.0" promptText="Password" />
                <Button fx:id="logoutButtonId" layoutX="130.0" layoutY="87.0" mnemonicParsing="false" onAction="#onLogout" text="Logout !" />
            </children></AnchorPane>
    </children>
</VBox>
